{
  "manifest_version": 2,
  "name": "Test",
  "version": "1",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.jpg"
  },
  "browser_action": {
    "default_icon": {
      "19": "a/icon-19.png",
      "38": "img.jpg"
    },
    "default_title": "Title",
    "default_popup": "popup.html"
  },
  "author": "Steven Roberts",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "minimum_chrome_version": "41.0",
  "offline_enabled": true,
  "permissions": [
    "contextMenus"
  ]
}
